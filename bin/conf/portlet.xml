<?xml version="1.0"?>

<portlet-app>

    <portlet>
        <portlet-name>EXT_GA_DASHBOARD</portlet-name>
        <display-name>Google Analytics Dashboard</display-name>
        <portlet-class>com.liferay.portlet.VelocityPortlet</portlet-class>
        <init-param>
            <name>view-template</name>
            <value>/ext/view.vtl</value>
        </init-param>
        <expiration-cache>0</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
        </supports>
        <resource-bundle>com.liferay.portlet.StrutsResourceBundle</resource-bundle>
        <security-role-ref>
            <role-name>CMS User</role-name>
        </security-role-ref>
    </portlet>


</portlet-app>